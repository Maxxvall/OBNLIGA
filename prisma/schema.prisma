generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model achievement_jobs {
  achievement_job_id BigInt               @id @default(autoincrement())
  status             AchievementJobStatus @default(PENDING)
  payload            Json
  attempts           Int                  @default(0)
  error              String?
  created_at         DateTime             @default(now())
  updated_at         DateTime

  @@index([status, created_at], map: "achievement_job_status_created_idx")
}

model achievement_level {
  achievement_level_id Int              @id @default(autoincrement())
  achievement_id       Int
  level                Int
  threshold            Int
  icon_url             String?
  title                String
  description          String?
  created_at           DateTime         @default(now())
  updated_at           DateTime
  achievement_type     achievement_type @relation(fields: [achievement_id], references: [achievement_type_id], onDelete: Cascade)

  @@unique([achievement_id, level])
}

model achievement_progress {
  achievement_progress_id BigInt           @id @default(autoincrement())
  user_id                 Int
  achievement_id          Int
  current_level           Int              @default(0)
  progress_count          Int              @default(0)
  last_unlocked_at        DateTime?
  created_at              DateTime         @default(now())
  updated_at              DateTime
  achievement_type        achievement_type @relation(fields: [achievement_id], references: [achievement_type_id], onDelete: Cascade)
  app_user                app_user         @relation(fields: [user_id], references: [user_id], onDelete: Cascade)

  @@unique([user_id, achievement_id])
}

model achievement_type {
  achievement_type_id  Int                    @id @default(autoincrement())
  name                 String
  description          String?
  required_value       Int
  metric               AchievementMetric
  created_at           DateTime               @default(now())
  updated_at           DateTime
  achievement_level    achievement_level[]
  achievement_progress achievement_progress[]
  user_achievement     user_achievement[]
}

model ad_banner {
  ad_banner_id  BigInt    @id @default(autoincrement())
  title         String    @db.VarChar(80)
  subtitle      String?   @db.VarChar(160)
  target_url    String?
  image_data    Bytes
  image_mime    String
  image_width   Int
  image_height  Int
  image_size    Int
  display_order Int       @default(0)
  is_active     Boolean   @default(true)
  starts_at     DateTime?
  ends_at       DateTime?
  created_at    DateTime  @default(now())
  updated_at    DateTime

  @@index([is_active, display_order, updated_at], map: "ad_banner_active_order")
}

model admin_action_log {
  admin_action_log_id BigInt    @id @default(autoincrement())
  user_id             Int?
  admin_identifier    String
  action_type         String
  target_type         String?
  target_id           String?
  metadata            Json?
  created_at          DateTime  @default(now())
  expires_at          DateTime?
  app_user            app_user? @relation(fields: [user_id], references: [user_id])

  @@index([created_at], map: "admin_action_log_created_idx")
}

model admin_point_adjustment {
  point_adjustment_id BigInt       @id @default(autoincrement())
  user_id             Int
  admin_identifier    String
  delta               Int
  scope               RatingScope?
  reason              String?
  match_id            BigInt?
  created_at          DateTime     @default(now())
  updated_at          DateTime
  app_user            app_user     @relation(fields: [user_id], references: [user_id], onDelete: Cascade)

  @@index([user_id, created_at], map: "point_adjustment_user_created_idx")
}

model app_user {
  user_id                    Int                        @id @default(autoincrement())
  telegram_id                BigInt                     @unique
  username                   String?
  first_name                 String?
  photo_url                  String?
  registration_date          DateTime                   @default(now())
  last_login_date            DateTime?
  current_streak             Int                        @default(0)
  total_predictions          Int                        @default(0)
  league_player_status       LeaguePlayerStatus         @default(NONE)
  league_player_requested_at DateTime?
  league_player_verified_at  DateTime?
  league_player_id           Int?
  created_at                 DateTime                   @default(now())
  updated_at                 DateTime
  achievement_progress       achievement_progress[]
  admin_action_log           admin_action_log[]
  admin_point_adjustment     admin_point_adjustment[]
  person                     person?                    @relation(fields: [league_player_id], references: [person_id])
  daily_reward_claim         daily_reward_claim[]
  prediction                 prediction[]
  prediction_entry           prediction_entry[]
  prediction_streak          prediction_streak?
  rating_snapshot            rating_snapshot[]
  shop_order                 shop_order[]
  user_achievement           user_achievement[]
  user_achievement_rewards   user_achievement_rewards[]
  user_rating                user_rating?
}

model club {
  club_id                                        Int                        @id @default(autoincrement())
  name                                           String
  short_name                                     String
  logo_url                                       String?
  created_at                                     DateTime                   @default(now())
  updated_at                                     DateTime
  club_player                                    club_player[]
  club_season_stats                              club_season_stats[]
  disqualification                               disqualification[]
  match_match_away_team_idToclub                 match[]                    @relation("match_away_team_idToclub")
  match_match_home_team_idToclub                 match[]                    @relation("match_home_team_idToclub")
  match_event                                    match_event[]
  match_lineup                                   match_lineup[]
  match_series_match_series_away_club_idToclub   match_series[]             @relation("match_series_away_club_idToclub")
  match_series_match_series_home_club_idToclub   match_series[]             @relation("match_series_home_club_idToclub")
  match_series_match_series_winner_club_idToclub match_series[]             @relation("match_series_winner_club_idToclub")
  match_statistic                                match_statistic[]
  player_club_career_stats                       player_club_career_stats[]
  player_season_stats                            player_season_stats[]
  season_group_slot                              season_group_slot[]
  season_participant                             season_participant[]
  season_roster                                  season_roster[]
}

model club_player {
  club_id              Int
  person_id            Int
  default_shirt_number Int?
  created_at           DateTime @default(now())
  updated_at           DateTime
  club                 club     @relation(fields: [club_id], references: [club_id], onDelete: Cascade)
  person               person   @relation(fields: [person_id], references: [person_id], onDelete: Cascade)

  @@id([club_id, person_id])
}

model club_season_stats {
  season_id     Int
  club_id       Int
  points        Int      @default(0)
  wins          Int      @default(0)
  losses        Int      @default(0)
  goals_for     Int      @default(0)
  goals_against Int      @default(0)
  updated_at    DateTime @default(now())
  club          club     @relation(fields: [club_id], references: [club_id], onDelete: Cascade)
  season        season   @relation(fields: [season_id], references: [season_id], onDelete: Cascade)

  @@id([season_id, club_id])
}

model competition {
  competition_id Int             @id @default(autoincrement())
  name           String
  type           CompetitionType
  series_format  SeriesFormat
  created_at     DateTime        @default(now())
  updated_at     DateTime
  season         season[]
}

model daily_reward_claim {
  daily_reward_claim_id BigInt   @id @default(autoincrement())
  user_id               Int
  day_number            Int
  streak_after          Int
  points_awarded        Int
  claim_date_key        String   @db.VarChar(16)
  animation_key         String   @db.VarChar(32)
  claimed_at            DateTime @default(now())
  metadata              Json?
  app_user              app_user @relation(fields: [user_id], references: [user_id], onDelete: Cascade)

  @@unique([user_id, claim_date_key], map: "daily_reward_unique_day")
  @@index([claim_date_key], map: "daily_reward_date_idx")
}

model disqualification {
  disqualification_id  BigInt                 @id @default(autoincrement())
  person_id            Int
  club_id              Int?
  reason               DisqualificationReason
  sanction_date        DateTime
  ban_duration_matches Int
  matches_missed       Int                    @default(0)
  is_active            Boolean                @default(true)
  created_at           DateTime               @default(now())
  updated_at           DateTime
  club                 club?                  @relation(fields: [club_id], references: [club_id])
  person               person                 @relation(fields: [person_id], references: [person_id], onDelete: Cascade)
}

model match {
  match_id                      BigInt                @id @default(autoincrement())
  season_id                     Int?
  series_id                     BigInt?
  series_match_number           Int?
  match_date_time               DateTime
  home_team_id                  Int
  away_team_id                  Int
  home_score                    Int                   @default(0)
  away_score                    Int                   @default(0)
  status                        MatchStatus
  stadium_id                    Int?
  referee_id                    Int?
  round_id                      Int?
  group_id                      Int?
  is_archived                   Boolean               @default(false)
  has_penalty_shootout          Boolean               @default(false)
  penalty_home_score            Int                   @default(0)
  penalty_away_score            Int                   @default(0)
  broadcast_url                 String?
  event_name                    String?
  is_friendly                   Boolean               @default(false)
  created_at                    DateTime              @default(now())
  updated_at                    DateTime
  club_match_away_team_idToclub club                  @relation("match_away_team_idToclub", fields: [away_team_id], references: [club_id])
  season_group                  season_group?         @relation(fields: [group_id], references: [season_group_id])
  club_match_home_team_idToclub club                  @relation("match_home_team_idToclub", fields: [home_team_id], references: [club_id])
  person                        person?               @relation(fields: [referee_id], references: [person_id])
  season_round                  season_round?         @relation(fields: [round_id], references: [round_id])
  season                        season?               @relation(fields: [season_id], references: [season_id], onDelete: Cascade)
  match_series                  match_series?         @relation(fields: [series_id], references: [series_id])
  stadium                       stadium?              @relation(fields: [stadium_id], references: [stadium_id])
  match_event                   match_event[]
  match_lineup                  match_lineup[]
  match_statistic               match_statistic[]
  prediction                    prediction[]
  prediction_template           prediction_template[]

  @@unique([series_id, series_match_number], map: "unique_series_match_number")
}

model match_event {
  event_id                                     BigInt         @id @default(autoincrement())
  match_id                                     BigInt
  team_id                                      Int
  minute                                       Int
  event_type                                   MatchEventType
  player_id                                    Int
  related_player_id                            Int?
  created_at                                   DateTime       @default(now())
  updated_at                                   DateTime
  match                                        match          @relation(fields: [match_id], references: [match_id], onDelete: Cascade)
  person_match_event_player_idToperson         person         @relation("match_event_player_idToperson", fields: [player_id], references: [person_id])
  person_match_event_related_player_idToperson person?        @relation("match_event_related_player_idToperson", fields: [related_player_id], references: [person_id])
  club                                         club           @relation(fields: [team_id], references: [club_id])
}

model match_lineup {
  match_id  BigInt
  person_id Int
  club_id   Int
  role      LineupRole
  position  String?
  club      club       @relation(fields: [club_id], references: [club_id])
  match     match      @relation(fields: [match_id], references: [match_id], onDelete: Cascade)
  person    person     @relation(fields: [person_id], references: [person_id])

  @@id([match_id, person_id])
}

model match_series {
  series_id                              BigInt       @id @default(autoincrement())
  season_id                              Int
  stage_name                             String
  home_club_id                           Int
  away_club_id                           Int
  series_status                          SeriesStatus
  winner_club_id                         Int?
  home_seed                              Int?
  away_seed                              Int?
  bracket_slot                           Int?
  created_at                             DateTime     @default(now())
  updated_at                             DateTime
  match                                  match[]
  club_match_series_away_club_idToclub   club         @relation("match_series_away_club_idToclub", fields: [away_club_id], references: [club_id])
  club_match_series_home_club_idToclub   club         @relation("match_series_home_club_idToclub", fields: [home_club_id], references: [club_id])
  season                                 season       @relation(fields: [season_id], references: [season_id], onDelete: Cascade)
  club_match_series_winner_club_idToclub club?        @relation("match_series_winner_club_idToclub", fields: [winner_club_id], references: [club_id])
}

model match_statistic {
  match_id        BigInt
  club_id         Int
  total_shots     Int      @default(0)
  shots_on_target Int      @default(0)
  corners         Int      @default(0)
  yellow_cards    Int      @default(0)
  red_cards       Int      @default(0)
  created_at      DateTime @default(now())
  updated_at      DateTime
  club            club     @relation(fields: [club_id], references: [club_id], onDelete: Cascade)
  match           match    @relation(fields: [match_id], references: [match_id], onDelete: Cascade)

  @@id([match_id, club_id])
}

model news {
  news_id          BigInt   @id @default(autoincrement())
  title            String   @db.VarChar(100)
  content          String
  cover_url        String?
  send_to_telegram Boolean  @default(false)
  created_at       DateTime @default(now())

  @@index([created_at(sort: Desc)], map: "news_created_at_desc")
}

model person {
  person_id                                         Int                        @id @default(autoincrement())
  first_name                                        String
  last_name                                         String
  is_player                                         Boolean                    @default(true)
  created_at                                        DateTime                   @default(now())
  updated_at                                        DateTime
  app_user                                          app_user[]
  club_player                                       club_player[]
  disqualification                                  disqualification[]
  match                                             match[]
  match_event_match_event_player_idToperson         match_event[]              @relation("match_event_player_idToperson")
  match_event_match_event_related_player_idToperson match_event[]              @relation("match_event_related_player_idToperson")
  match_lineup                                      match_lineup[]
  player_club_career_stats                          player_club_career_stats[]
  player_season_stats                               player_season_stats[]
  season_roster                                     season_roster[]
}

model player_club_career_stats {
  person_id     Int
  club_id       Int
  total_goals   Int    @default(0)
  penalty_goals Int    @default(0)
  total_matches Int    @default(0)
  total_assists Int    @default(0)
  yellow_cards  Int    @default(0)
  red_cards     Int    @default(0)
  club          club   @relation(fields: [club_id], references: [club_id], onDelete: Cascade)
  person        person @relation(fields: [person_id], references: [person_id], onDelete: Cascade)

  @@id([person_id, club_id])
}

model player_season_stats {
  season_id      Int
  person_id      Int
  club_id        Int
  goals          Int      @default(0)
  penalty_goals  Int      @default(0)
  assists        Int      @default(0)
  yellow_cards   Int      @default(0)
  red_cards      Int      @default(0)
  matches_played Int      @default(0)
  updated_at     DateTime @default(now())
  club           club     @relation(fields: [club_id], references: [club_id], onDelete: Cascade)
  person         person   @relation(fields: [person_id], references: [person_id], onDelete: Cascade)
  season         season   @relation(fields: [season_id], references: [season_id], onDelete: Cascade)

  @@id([season_id, person_id])
}

model prediction {
  prediction_id    BigInt            @id @default(autoincrement())
  user_id          Int
  match_id         BigInt
  prediction_date  DateTime
  result_1x2       PredictionResult?
  total_goals_over Float?
  penalty_yes      Boolean?
  red_card_yes     Boolean?
  is_correct       Boolean?
  points_awarded   Int               @default(0)
  created_at       DateTime          @default(now())
  updated_at       DateTime
  match            match             @relation(fields: [match_id], references: [match_id], onDelete: Cascade)
  app_user         app_user          @relation(fields: [user_id], references: [user_id], onDelete: Cascade)

  @@unique([user_id, match_id])
}

model prediction_entry {
  prediction_entry_id BigInt                @id @default(autoincrement())
  template_id         BigInt
  user_id             Int
  selection           String
  submitted_at        DateTime              @default(now())
  score_awarded       Int?
  status              PredictionEntryStatus @default(PENDING)
  resolution_meta     Json?
  resolved_at         DateTime?
  created_at          DateTime              @default(now())
  prediction_template prediction_template   @relation(fields: [template_id], references: [prediction_template_id], onDelete: Cascade)
  app_user            app_user              @relation(fields: [user_id], references: [user_id], onDelete: Cascade)

  @@unique([user_id, template_id], map: "prediction_entry_user_template_unique")
  @@index([template_id, status], map: "prediction_entry_template_status_idx")
}

model prediction_streak {
  user_id            Int       @id
  current_streak     Int       @default(0)
  max_streak         Int       @default(0)
  last_prediction_at DateTime?
  last_resolved_at   DateTime?
  updated_at         DateTime
  created_at         DateTime  @default(now())
  app_user           app_user  @relation(fields: [user_id], references: [user_id], onDelete: Cascade)
}

model prediction_template {
  prediction_template_id BigInt               @id @default(autoincrement())
  match_id               BigInt
  market_type            PredictionMarketType
  options                Json
  base_points            Int                  @default(0)
  difficulty_multiplier  Decimal              @default(1.0)
  is_manual              Boolean              @default(false)
  created_by             String?
  created_at             DateTime             @default(now())
  updated_at             DateTime
  prediction_entry       prediction_entry[]
  match                  match                @relation(fields: [match_id], references: [match_id], onDelete: Cascade)

  @@index([match_id, market_type], map: "prediction_template_match_market_idx")
}

model rating_season {
  rating_season_id     BigInt                 @id @default(autoincrement())
  scope                RatingScope
  starts_at            DateTime
  ends_at              DateTime
  closed_at            DateTime?
  duration_days        Int
  created_at           DateTime               @default(now())
  updated_at           DateTime
  rating_season_winner rating_season_winner[]

  @@index([scope, closed_at], map: "rating_season_scope_closed_idx")
  @@index([scope, starts_at], map: "rating_season_scope_start_idx")
}

model rating_season_winner {
  rating_season_winner_id BigInt        @id @default(autoincrement())
  season_id               BigInt
  user_id                 Int
  rank                    Int
  scope_points            Int
  total_points            Int
  prediction_count        Int           @default(0)
  prediction_wins         Int           @default(0)
  display_name            String
  username                String?
  photo_url               String?
  created_at              DateTime      @default(now())
  rating_season           rating_season @relation(fields: [season_id], references: [rating_season_id], onDelete: Cascade)

  @@unique([season_id, rank], map: "rating_season_winner_season_rank_unique")
  @@index([user_id, season_id], map: "rating_season_winner_user_idx")
}

model rating_settings {
  rating_settings_id Int      @id @default(autoincrement())
  current_scope_days Int
  yearly_scope_days  Int
  created_at         DateTime @default(now())
  updated_at         DateTime
}

model rating_snapshot {
  rating_snapshot_id BigInt      @id @default(autoincrement())
  user_id            Int
  scope              RatingScope
  rank               Int
  points             Int
  captured_at        DateTime
  payload            Json?
  app_user           app_user    @relation(fields: [user_id], references: [user_id], onDelete: Cascade)

  @@index([scope, captured_at], map: "rating_snapshot_scope_captured_idx")
}

model season {
  season_id           Int                   @id @default(autoincrement())
  competition_id      Int
  name                String
  start_date          DateTime
  end_date            DateTime
  city                String?
  series_format       SeriesFormat?
  is_active           Boolean               @default(false)
  created_at          DateTime              @default(now())
  updated_at          DateTime
  club_season_stats   club_season_stats[]
  match               match[]
  match_series        match_series[]
  player_season_stats player_season_stats[]
  competition         competition           @relation(fields: [competition_id], references: [competition_id])
  season_group        season_group[]
  season_participant  season_participant[]
  season_roster       season_roster[]
  season_round        season_round[]
}

model season_group {
  season_group_id   Int                 @id @default(autoincrement())
  season_id         Int
  group_index       Int
  label             String
  qualify_count     Int
  created_at        DateTime            @default(now())
  updated_at        DateTime
  match             match[]
  season            season              @relation(fields: [season_id], references: [season_id], onDelete: Cascade)
  season_group_slot season_group_slot[]
  season_round      season_round[]

  @@unique([season_id, group_index])
}

model season_group_slot {
  season_group_slot_id Int          @id @default(autoincrement())
  group_id             Int
  position             Int
  club_id              Int?
  created_at           DateTime     @default(now())
  updated_at           DateTime
  club                 club?        @relation(fields: [club_id], references: [club_id])
  season_group         season_group @relation(fields: [group_id], references: [season_group_id], onDelete: Cascade)

  @@unique([group_id, position])
}

model season_participant {
  season_id Int
  club_id   Int
  club      club   @relation(fields: [club_id], references: [club_id])
  season    season @relation(fields: [season_id], references: [season_id], onDelete: Cascade)

  @@id([season_id, club_id])
}

model season_roster {
  season_id         Int
  club_id           Int
  person_id         Int
  shirt_number      Int
  registration_date DateTime
  club              club     @relation(fields: [club_id], references: [club_id], onDelete: Cascade)
  person            person   @relation(fields: [person_id], references: [person_id])
  season            season   @relation(fields: [season_id], references: [season_id], onDelete: Cascade)

  @@id([season_id, club_id, person_id])
  @@unique([season_id, club_id, shirt_number])
}

model season_round {
  round_id     Int           @id @default(autoincrement())
  season_id    Int
  round_type   RoundType
  round_number Int?
  label        String
  group_id     Int?
  created_at   DateTime      @default(now())
  updated_at   DateTime
  match        match[]
  season_group season_group? @relation(fields: [group_id], references: [season_group_id])
  season       season        @relation(fields: [season_id], references: [season_id], onDelete: Cascade)

  @@unique([season_id, label])
}

model shop_item {
  shop_item_id    Int               @id @default(autoincrement())
  slug            String?           @unique(map: "shop_item_slug_unique")
  title           String            @db.VarChar(80)
  subtitle        String?           @db.VarChar(160)
  description     String?
  price_cents     Int
  currency_code   String            @default("RUB")
  stock_quantity  Int?
  max_per_order   Int               @default(3)
  sort_order      Int               @default(100)
  is_active       Boolean           @default(true)
  image_url       String?
  image_data      Bytes?
  image_mime      String?
  image_width     Int?
  image_height    Int?
  image_size      Int?
  created_at      DateTime          @default(now())
  updated_at      DateTime
  shop_order_item shop_order_item[]

  @@index([is_active, sort_order], map: "shop_item_active_order_idx")
}

model shop_order {
  shop_order_id   BigInt            @id @default(autoincrement())
  order_number    String            @unique
  user_id         Int?
  telegram_id     BigInt?
  username        String?
  first_name      String?
  status          ShopOrderStatus   @default(PENDING)
  total_cents     Int
  currency_code   String            @default("RUB")
  customer_note   String?
  confirmed_at    DateTime?
  confirmed_by    String?
  created_at      DateTime          @default(now())
  updated_at      DateTime
  app_user        app_user?         @relation(fields: [user_id], references: [user_id])
  shop_order_item shop_order_item[]

  @@index([status, created_at], map: "shop_order_status_created_idx")
}

model shop_order_item {
  shop_order_item_id BigInt     @id @default(autoincrement())
  order_id           BigInt
  item_id            Int
  title              String     @db.VarChar(80)
  subtitle           String?    @db.VarChar(160)
  price_cents        Int
  quantity           Int
  image_url          String?
  shop_item          shop_item  @relation(fields: [item_id], references: [shop_item_id])
  shop_order         shop_order @relation(fields: [order_id], references: [shop_order_id], onDelete: Cascade)

  @@index([item_id], map: "shop_order_item_item_idx")
  @@index([order_id], map: "shop_order_item_order_idx")
}

model stadium {
  stadium_id Int      @id @default(autoincrement())
  name       String
  city       String
  created_at DateTime @default(now())
  updated_at DateTime
  match      match[]
}

model user_achievement {
  user_id             Int
  achievement_type_id Int
  achieved_date       DateTime
  achievement_type    achievement_type @relation(fields: [achievement_type_id], references: [achievement_type_id], onDelete: Cascade)
  app_user            app_user         @relation(fields: [user_id], references: [user_id], onDelete: Cascade)

  @@id([user_id, achievement_type_id])
}

model user_achievement_rewards {
  user_achievement_reward_id BigInt   @id @default(autoincrement())
  user_id                    Int
  group                      String
  tier                       Int
  season_id                  Int?
  points                     Int
  notified                   Boolean  @default(false)
  created_at                 DateTime @default(now())
  app_user                   app_user @relation(fields: [user_id], references: [user_id], onDelete: Cascade)

  @@unique([user_id, group, tier, season_id])
  @@index([user_id, notified], map: "user_achievement_reward_user_notified_idx")
}

model user_rating {
  user_id              Int         @id
  total_points         Int         @default(0)
  seasonal_points      Int         @default(0)
  yearly_points        Int         @default(0)
  mythic_rank          Int?
  current_level        RatingLevel @default(BRONZE)
  prediction_count     Int         @default(0)
  prediction_wins      Int         @default(0)
  last_recalculated_at DateTime?
  created_at           DateTime    @default(now())
  updated_at           DateTime
  app_user             app_user    @relation(fields: [user_id], references: [user_id], onDelete: Cascade)
}

